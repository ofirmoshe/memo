{
  "expo": {
    "name": "Memora",
    "slug": "MemoraTest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.memora.assistant",
      "icon": "./assets/icon.png",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "memora",
            "CFBundleURLSchemes": ["memora"]
          },
          {
            "CFBundleURLName": "google",
            "CFBundleURLSchemes": ["com.memora.assistant"]
          }
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeName": "Web URL",
            "CFBundleTypeRole": "Editor",
            "LSItemContentTypes": ["public.url"],
            "LSHandlerRank": "Owner"
          },
          {
            "CFBundleTypeName": "Plain Text",
            "CFBundleTypeRole": "Editor",
            "LSItemContentTypes": ["public.text", "public.plain-text"],
            "LSHandlerRank": "Owner"
          }
        ],
        "UTImportedTypeDeclarations": [
          {
            "UTTypeIdentifier": "public.url",
            "UTTypeDescription": "Web URL",
            "UTTypeConformsTo": ["public.data"],
            "UTTypeTagSpecification": {
              "public.filename-extension": ["url"],
              "public.mime-type": ["text/uri-list"]
            }
          }
        ]
      },
      "associatedDomains": ["applinks:memora.app"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.memora.assistant",
      "icon": "./assets/icon.png",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "memora"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        },
        {
          "action": "SEND",
          "data": [
            {
              "mimeType": "text/plain"
            }
          ],
          "category": ["DEFAULT"]
        },
        {
          "action": "SEND",
          "data": [
            {
              "mimeType": "text/*"
            }
          ],
          "category": ["DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "memora",
    "extra": {
      "eas": {
        "projectId": "91627cdc-34c9-4cb9-95c1-34fc72d9eecc"
      }
    },
    "owner": "ofirmoshe"
  }
}
