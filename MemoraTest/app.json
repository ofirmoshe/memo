{
  "expo": {
    "name": "Memora",
    "slug": "MemoraTest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.memora.assistant",
      "icon": "./assets/icon.png",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "memora",
            "CFBundleURLSchemes": [
              "memora"
            ]
          },
          {
            "CFBundleURLName": "google",
            "CFBundleURLSchemes": [
              "com.memora.assistant"
            ]
          }
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Memora needs access to your photo library to save images you want to remember.",
        "NSCameraUsageDescription": "Memora needs camera access to take photos you want to save to your memory.",
        "NSMicrophoneUsageDescription": "Memora needs microphone access to record voice notes.",
        "NSUserTrackingUsageDescription": "This allows us to provide you with personalized content and improve your experience."
      },
      "associatedDomains": [
        "applinks:memora.app"
      ],
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.memora.assistant"
        ]
      },
      "buildNumber": "2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.memora.assistant",
      "icon": "./assets/icon.png",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "memora"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "SEND",
          "data": [
            {
              "mimeType": "text/plain"
            }
          ],
          "category": [
            "DEFAULT"
          ]
        },
        {
          "action": "SEND",
          "data": [
            {
              "mimeType": "text/*"
            }
          ],
          "category": [
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "memora",
    "extra": {
      "eas": {
        "projectId": "91627cdc-34c9-4cb9-95c1-34fc72d9eecc"
      }
    },
    "owner": "ofirmoshe",
    "plugins": [
      [
        "expo-share-intent",
        {
          "iosActivationRules": {
            "NSExtensionActivationSupportsWebURLWithMaxCount": 10,
            "NSExtensionActivationSupportsText": true
          },
          "iosAppGroupIdentifier": "group.com.memora.assistant"
        }
      ]
    ]
  }
}
